{
  "name": "kraye",
  "private": true,
  "version": "4.24.0",
  "description": "Minimal Mistakes 2 column Jekyll theme.",
  "repository": {
    "type": "git",
    "url": "git://github.com/mmistakes/minimal-mistakes.git"
  },
  "dependencies": {
    "acorn": "^8.12.0",
    "ajv": "^6.12.6",
    "ansi-styles": "^3.2.1",
    "chalk": "^2.4.2",
    "cross-spawn": "^6.0.5",
    "debug": "^4.3.5",
    "eslint-scope": "^8.0.1",
    "espree": "^10.0.1",
    "find-up": "^5.0.0",
    "glob": "^7.1.7",
    "globals": "^14.0.0",
    "has-flag": "^3.0.0",
    "import-fresh": "^3.3.0",
    "lodash": "^4.17.21",
    "minimatch": "^3.1.2",
    "mkdirp": "^1.0.4",
    "natural-compare": "^1.4.0",
    "node-libs-browser": "^2.2.1",
    "node-releases": "^2.0.14",
    "resolve": "^1.22.8",
    "semver": "^5.7.2",
    "source-map": "^0.6.1",
    "supports-color": "^5.5.0",
    "validate-npm-package-license": "^3.0.4",
    "vercel": "^32.3.0",
    "webpack-merge": "^5.10.0"
  },
  "keywords": [
    "jekyll",
    "theme",
    "minimal"
  ],
  "author": "Michael Rose",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmistakes/minimal-mistakes/issues"
  },
  "homepage": "https://mmistakes.github.io/minimal-mistakes/",
  "engines": {
    "node": "20.x"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "babel-loader": "^9.1.3",
    "chokidar": "^3.6.0",
    "eslint": "^9.4.0",
    "eslint-plugin-es": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "uglify-js": "^3.13.6",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "uglify": "uglifyjs assets/js/vendor/jquery/jquery-3.6.0.js assets/js/plugins/jquery.fitvids.js assets/js/plugins/jquery.greedy-navigation.js assets/js/plugins/jquery.magnific-popup.js assets/js/plugins/jquery.ba-throttle-debounce.js assets/js/plugins/smooth-scroll.js assets/js/plugins/gumshoe.js assets/js/_main.js -c -m -o assets/js/main.min.js",
    "add-banner": "node banner.js",
    "watch:js": "onchange \"assets/js/**/*.js\" -e \"assets/js/main.min.js\" -- npm run build:js",
    "build:js": "npm run uglify && npm run add-banner",
    "build": "npm run lint && npm run compile && npm run bundle",
    "vercel-build": "npm run build && cp -r build/ _site/",
    "vercel": "vercel --prod",
    "lint": "eslint .",
    "compile": "webpack",
    "bundle": "webpack --config webpack.config.js"
  },
  "main": ".eslintrc.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  }
}
