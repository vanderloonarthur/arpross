{
  "name": "kraye",
  "private": true,
  "version": "4.24.0",
  "description": "Minimal Mistakes 2 column Jekyll theme.",
  "repository": {
    "type": "git",
    "url": "git://github.com/mmistakes/minimal-mistakes.git"
  },
  "dependencies": {
    "uglify-js": "^3.14.1",
    "cssnano": "^4.1.11",
    "eslint": "^7.32.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "onchange": "^7.1.0"
  },
  "keywords": [
    "jekyll",
    "theme",
    "minimal"
  ],
  "author": "Michael Rose",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmistakes/minimal-mistakes/issues"
  },
  "homepage": "https://mmistakes.github.io/minimal-mistakes/",
  "engines": {
    "node": "20.x"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "babel-loader": "^9.1.3",
    "chokidar": "^3.6.0",
    "eslint": "^9.4.0",
    "eslint-plugin-es": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "uglify-js": "^3.13.6",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
  "uglify": "npx uglifyjs assets/js/vendor/jquery/jquery-3.6.0.js assets/js/plugins/jquery.fitvids.js assets/js/plugins/jquery.greedy-navigation.js assets/js/plugins/jquery.magnific-popup.js assets/js/plugins/jquery.ba-throttle-debounce.js assets/js/plugins/smooth-scroll.js assets/js/plugins/gumshoe.js assets/js/_main.js -c -m -o assets/js/main.min.js",
  "add-banner": "node banner.js",
  "watch:js": "npx onchange \"assets/js/**/*.js\" -e \"assets/js/main.min.js\" -- npm run build:js",
  "build:js": "npm run uglify && npm run add-banner",
  "build:css": "npx cssnano assets/css/**/*.css -o assets/css/main.min.css",
  "build": "npm run build:js && npm run build:css && npm run lint && npm run compile && npm run bundle",
  "vercel-build": "npm run build && cp -r build/ _site/",
  "vercel": "npx vercel --prod",
  "lint": "npx eslint .",
  "compile": "npx webpack",
  "bundle": "npx webpack --config webpack.config.js"
},
  "main": ".eslintrc.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  }
}
